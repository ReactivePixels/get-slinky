/*
 * Swanky Site Theme
 * Branding
 */

 .brand
  width 168px
  height 153px
  display block
  margin: 0 auto
  padding-top 65px
  position relative
  z-index 10

  .logo
    width 100%
    height 100%
    background url('../img/logo.svg') no-repeat
    background-size @width @height

   &:hover, &:focus
    text-decoration none
